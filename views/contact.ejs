<% include header.ejs %>

<div class="panel panel-primary">
	
	<form method="POST" action="contact" role="form">
		<div class="panel-body">
			<div class="form-group">
				<label for="emailID">Enter Email ID</label>
				<input type="text" class="form-control" id="emailID"
				  name="emailID" placeholder="Enter Your EmailID" required>
			</div>


			<div class="form-group">
			<label for="content">Enter Your Message</label>
			<textarea class="form-control" id="body" name="body"
			placeholder="write your message to admin" rows="3" required></textarea></div>
			<div class="form-group">
				<input type="submit" value="Submit" class="btn btn-primary">
			</div>
	</div>
	</form>
</div>
<% include footer.ejs %>
